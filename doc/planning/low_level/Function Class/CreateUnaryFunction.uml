@startuml
title Make Function Flow Diagram

partition Unary {

:Parse Expression;
if(Leaf) then(yes)
    :Add as head node
    to Function;
    end
else
    :Add op as head node
    to Function;
    repeat
        :Make Node for e;
        :Attach e to @ op;
    repeat while(e not a leaf)
endif
end
}

@enduml